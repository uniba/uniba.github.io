  <body>

    <div id="events">

      <div id="wrap-inner">

        <header id="header">

          <h1 class="background-blue"><a href="https://uniba.jp">UNIBA INC.</a></h1>
          <div class="clearfix"></div>
          <nav id="glb-nav">
            <ul>
              <li class="background-blue"><a href="home.html">HOME</a></li>
              <li class="background-blue"><a href="about.html">ABOUT</a></li>
              <li class="background-blue"><a href="service.html">SERVICE</a></li>
