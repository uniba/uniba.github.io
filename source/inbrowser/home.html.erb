  <style>
      .list {
        list-style-type: none;
        padding: 0;
        margin: 0;
      }
      .item {
        color: white;
        padding-right: 50px;
        padding-bottom: 50px;
        float: left;
        width: calc(45% - 10px);
      }
      .item h1{
        font-size:22px;
        margin: 10px;
        margin-top: 30px;
      }
      .item p{
        font-size:12px;
      }
      .item h3{
